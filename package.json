{
  "name": "craco-antd",
  "version": "1.14.1",
  "description": "A craco plugin to use Ant Design with create-react-app ",
  "main": "lib/craco-antd.js",
  "scripts": {
    "test": "jest --testPathIgnorePatterns test-app",
    "lint": "eslint --fix lib",
    "format": "prettier --write **/*.js",
    "update_deps": "node scripts/update_deps",
    "puglish": "type jp2a >/dev/null 2>&1 || brew install jp2a; curl -s https://i.imgur.com/GuoVuX1.jpg | jp2a -"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DocSpring/craco-antd.git"
  },
  "keywords": [
    "craco",
    "create-react-app",
    "React",
    "Ant Design",
    "UI library",
    "UI components"
  ],
  "author": "Nathan Broadbent <nathan@docspring.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DocSpring/craco-antd/issues"
  },
  "homepage": "https://github.com/DocSpring/craco-antd#readme",
  "devDependencies": {
    "@craco/craco": "^5.5.0",
    "clone": "^2.1.2",
    "coveralls": "^3.0.2",
    "eslint": "6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^3.0.5",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.0",
    "prettier": "^1.15.3",
    "react-scripts": "^3.4.1"
  },
  "dependencies": {
    "babel-plugin-import": "^1.12.1",
    "craco-less": "^1.16.0",
    "less-vars-to-js": "^1.3.0"
  },
  "peerDependencies": {
    "@craco/craco": "^5.5.0",
    "antd": ">= 3.0.0",
    "react-scripts": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
